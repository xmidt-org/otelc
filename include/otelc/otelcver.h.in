#ifndef _OTELC_VERSION_H_
#define _OTELC_VERSION_H_ 1

#define OTELC_VERSION "@otelc_VERSION@"

#define OTELC_VERSION_MAJOR @otelc_VERSION_MAJOR @
#define OTELC_VERSION_MINOR @otelc_VERSION_MINOR @
#define OTELC_VERSION_PATCH @otelc_VERSION_PATCH @

/* This is the numeric version of the otelc version number, meant for easier
 * parsing and comparisons by programs.  The OTELC_VERSION_NUM define will
 * always follow this syntax:
 *
 *      0xAABBCC
 *
 * Where AA, BB, and CC are each major version, minor version, patch in hex
 * using 8 bits each.
 *
 * It should be safe to compare two different versions with the greater number
 * being the newer version.
 */
#define OTELC_VERSION_NUM @otelc_BITWISE_VERSION @

#endif
